#!/bin/bash
#

# Sync CM10.1 phone sources
cd ~/android/carbondev/
repo sync -j8

# Update halo & PA wallpapers
cd ~/android/carbondev/packages/apps/HALO
git pull
cd ~/android/carbondev/packages/apps/ParanoidWallpapers
git pull

# Return to rootbox
cd ~/android/carbondev/packages/apps/Camera
git checkout rootbox
cd ~/android/carbondev/packages/apps/Gallery2
git checkout rootbox
